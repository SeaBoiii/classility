{
  "name": "classility-rpg-classes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "export": "tsx scripts/export.ts",
    "export:all": "tsx scripts/export.ts --all",
    "tune:reachability": "tsx scripts/tune-reachability.ts --target-reachability 1 --timeout-ms 3600000 --workers 0",
    "tune:reachability:apply": "tsx scripts/tune-reachability.ts --target-reachability 1 --timeout-ms 3600000 --workers 0 --write",
    "tune:balance": "tsx scripts/tune-reachability.ts --target-reachability 1 --optimize-probability --probability-samples 40000 --probability-tolerance 1 --timeout-ms 3600000 --workers 0",
    "tune:balance:apply": "tsx scripts/tune-reachability.ts --target-reachability 1 --optimize-probability --probability-samples 40000 --probability-tolerance 1 --timeout-ms 3600000 --workers 0 --write",
    "tune:balance:apply-best": "tsx scripts/tune-reachability.ts --target-reachability 1 --optimize-probability --probability-samples 40000 --probability-tolerance 1 --timeout-ms 3600000 --workers 0 --write --write-best"
  },
  "dependencies": {
    "@fontsource/cinzel": "^5.2.8",
    "@fontsource/eb-garamond": "^5.2.7",
    "clsx": "^2.1.1",
    "html-to-image": "^1.11.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.4.1"
  }
}
